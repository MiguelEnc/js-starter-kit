{
  "name": "js-starter-kit",
  "version": "1.0.0",
  "description": "Base JavaScript Development Environment",
  "main": "index.js",
  "scripts": {
    "predev": "npm-run-all lint",
    "dev": "webpack-dev-server --config webpack.dev.js --open --hot",
    "open:src": "babel-node buildScripts/devServer.js",
    "lint": "esw webpack.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 5000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "jest",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiguelEnc/js-starter-kit.git"
  },
  "author": "Miguel Encarnacion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MiguelEnc/js-starter-kit/issues"
  },
  "homepage": "https://github.com/MiguelEnc/js-starter-kit#readme",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "compression": "^1.7.4",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "localtunnel": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.4",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-md5-hash": "0.0.6"
  }
}
